{
    "name": "black-bits/preview-thumbnails",
    "description": "Preview Thumbnail Builder for the Laravel Framework",
    "license": "MIT",
    "homepage": "https://github.com/black-bits/preview-thumbnails",
    "support": {
        "issues": "https://github.com/black-bits/preview-thumbnails/issues",
        "source": "https://github.com/black-bits/preview-thumbnails"
    },
    "authors": [
        {
            "name": "Oliver Heck",
            "email": "mail@oliverheck.de"
        },
        {
            "name": "Andreas Przywara",
            "email": "andreas.przywara@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "BlackBits\\PreviewThumbnails\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlackBits\\PreviewThumbnails\\PreviewThumbnailsServiceProvider"
            ],
            "aliases": {
                "PreviewThumbnails": "BlackBits\\PreviewThumbnails\\PreviewThumbnailsFacade"
            }
        }
    },
    "config": {
        "bin-dir": "vendor/bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
